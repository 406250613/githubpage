/* pages/details/details.wxss */
page{
  position: relative;
  height: 100%
}
.top2{
  height: 120rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.top2 .progressBox{
  width: 100%;
  position: relative;
  font-size: 24rpx;
  color: #878888;
  margin-top: 20rpx;
}
.top2 .progressBox .line{
  height: 1px;
  width: 80%;
  position: absolute;
  left: 10%;
  background: #E0E0E0;
  top: 16rpx;
  z-index: 1;
}
.top2 .progressBox .box{
  display: flex;
  justify-content: space-between;
  width: 90%;
  margin: 0 auto;
  /* position: absolute;
  top: 0 */
}
.top2 .progressBox .box .item{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center
}
.top2 .progressBox .box .item > view{
  width: 20rpx;
  height: 20rpx;
  background: #C8C8C8;
  border-radius: 50%;
  margin-bottom: 10rpx;
  position: relative;
  z-index: 10;
}

.top{
  height: 120rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.top .progressBox{
  width: 80%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  color: #E0E0E0;

}
.top .progressBox > view{
  width: 20rpx;
  height: 20rpx;
  background: #C8C8C8;
  border-radius: 50%;
}
.top .progressBox > view:nth-of-type(1):after{
  content: '待对接';
  font-size: 24rpx;
  color: #878888;
  width: 150rpx;
  display: block;
  position: relative;
  top: 28rpx;
  left: -26rpx;
}
.top .progressBox > view:nth-of-type(2):after{
  content: '对接中';
  font-size: 24rpx;
  color: #878888;
  width: 150rpx;
  display: block;
  position: relative;
  top: 28rpx;
  left: -26rpx;
}
.top .progressBox > view:nth-of-type(3):after{
  content: '待反馈';
  font-size: 24rpx;
  color: #878888;
  width: 150rpx;
  display: block;
  position: relative;
  top: 28rpx;
  left: -26rpx;
}
.top .progressBox > view:nth-of-type(4):after{
  content: '已结束';
  font-size: 24rpx;
  color: #878888;
  width: 150rpx;
  display: block;
  position: relative;
  top: 28rpx;
  left: -26rpx;
}
.active{
  /* background: #fff !important;
  border: 4px solid #0089FC;
  width: 12rpx !important;
  height: 12rpx !important; */
  color:  #0089FC;
}
.active > view{
  background: #fff !important;
  border: 4px solid #0089FC;
  width: 12rpx !important;
  height: 12rpx !important;
}
.section1{
  padding: 25rpx 25rpx 0px 25rpx;
  font-size: 34rpx;
  border-bottom: 1px solid #C8C8C8;
}
.section1 > text{
  display: block;
  font-size: 28rpx;
  color: #444444;
  margin-top: 14rpx;
}
.section1 .info{
  border-top: 1px solid #C8C8C8;
  margin-top: 20rpx;
  padding: 10rpx 0 5rpx 0;
}
.section1 .info view{
  font-size: 28rpx;
  color: #A7A7A7;
  padding: 10rpx 0;
}
.section1 .info view text{
  font-size: 28rpx;
  color: #3C3C3C;
}
.section1 .info view:nth-of-type(1) image{
  width: 26rpx;
  height: 30rpx;
  margin-right: 8rpx;
  position: relative;
  top: 4rpx;
}
.section1 .info view:nth-of-type(2) image{
  width: 24rpx;
  height: 28rpx;
  margin-right: 8rpx;
  position: relative;
  top: 4rpx;
}

.section1 .info view:nth-of-type(3) image{
  width: 24rpx;
  height: 28rpx;
  margin-right: 8rpx;
  position: relative;
  top: 4rpx;
}

.section2{
  padding: 25rpx 25rpx 10rpx 25rpx;
  font-size: 34rpx;
  border-bottom: 1px solid #C8C8C8;
}
.section2 > view{
  font-size: 26rpx;
  color: #515252;
  display: flex;
  align-items: center;
  padding: 10rpx 0;
}
.section2 image{
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
}
.section2 > view .payphone{
  width: 60rpx;
  height: 30rpx;
  background: #7075D3;
  color: #fff;
  border-radius: 5px;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 16rpx;
  margin-left: 20rpx;
}

.section3{
  padding: 25rpx 25rpx 10rpx 25rpx;
  font-size: 34rpx;
  border-bottom: 10px solid #F9F9F9;
}
.section3 .but1{
  padding: 6rpx 25rpx;
  background: #53C070;
  color: #fff;
  font-size: 26rpx;
  display: inline-flex;
  border-radius: 4px;
  margin-right: 30rpx;
}
.section3 .but2{
  padding: 6rpx 25rpx;
  background: #5175D5;
  color: #fff;
  font-size: 26rpx;
  display: inline-flex;
  border-radius: 4px;
}
.section3 > text{
  font-size: 30rpx;
  color: #5A5B5B;
  display: block;
  margin-top: 10rpx;
  margin-bottom: 15rpx;
}
.section4{
  padding: 25rpx 25rpx 0px 25rpx;
  font-size: 34rpx;
  margin-bottom: 110rpx;
}

.bot{
  background: lightgray;
  position: fixed;
  width: 100%;
  left: 0;
  bottom: 0;
  height: 110rpx;
  display: flex;
}
.bot .left{
  width: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 32rpx;
  color: #4766C2;
  background: #fff;
  box-shadow: 0 -4rpx 6rpx #EDEEEE;
}
.bot .right{
  width: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 32rpx;
  color: #fff;
  background: #4766C2;
}
.bot image{
  width: 35rpx;
  height: 35rpx;
  margin-right: 12rpx;
}
.section4 .progressBox2{
  display: flex;
  flex-wrap: wrap;
  margin-top: 25rpx;
}
.section4 .progressBox2 .left{
  border-right: 1px solid #E3E4E4;
  width: 20%;
  display: flex;
  flex-direction: column;
  font-size: 32rpx;
  text-align: right;
  padding: 16rpx 20rpx;
  box-sizing: border-box;
  position: relative;
}
.section4 .progressBox2 .right{
  width: 78%;
  display: flex;
  align-items: center;
  font-size: 28rpx;
  padding: 0 40rpx;
  box-sizing: border-box;
}
.section4 .progressBox2 .left text{
  font-size: 26rpx;
  color: #515252;
}
.section4 .progressBox2 .left > view{
  width: 20rpx;
  height: 20rpx;
  background: #C8C8C8;
  border-radius: 50%;
  position: absolute;
  right: -10rpx;
  top: 40rpx;
}
.active2{
  background: #fff !important;
  border: 4px solid #0089FC;
  width: 12rpx !important;
  height: 12rpx !important;
  right: -14rpx !important;
}

.bg{
  position: fixed;
  width: 100%;
  height: 100%;
  background: #000;
  opacity: .2;
  z-index: 10;
}
.output{
  width: 80%;
  height: 470rpx;
  background: #fff;
  position: fixed;
  left: 50%;
  margin-left: -40%;
  top: 45%;
  margin-top: -235rpx;
  z-index: 100;
  border-radius: 2px;
  display: flex;
 
}
.output .bottom{
  position: absolute;
  bottom: 0;
  left: 0;
  display: flex;
  width: 100%;
  height: 90rpx;
}
.output .bottom view{
  color: white;
  background: #477CEF;
  font-size: 32rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 50%;
  height: 100%;
  position: relative;
}
.output .mes{
  width: 100%;
  padding: 30rpx 38rpx;
 
}
.output .mes .title{
  font-size: 32rpx;
  color: #333;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.output .mes .title picker{
  flex: 1;
  border: 1px solid #EAEAEA;
  margin-left: 15rpx;
  padding: 8rpx 14rpx;
  color: #4C4C4C;
}
.output .mes .des{
  font-size: 24rpx;
  color: #4C4C4C;
  margin-top: 25rpx;
}
.output .mes .read{
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 22rpx;
  color: #489FFF;
  margin-top: 30rpx;
}
.output .mes .read checkbox{
  transform:scale(.55);
}
.title picker{
  position: relative
}
.title picker::after{
  content: 'V';
  position: absolute;
  right: 20rpx;
  top: 12rpx;
  color: #A7A7A7;
  font-size: 26rpx;
}

.shareRuleOutput{
  width: 90%;
  height: 500rpx;
  background: #fff;
  position: fixed;
  left: 50%;
  margin-left: -45%;
  top: 45%;
  margin-top: -260rpx;
  z-index: 100;
  border-radius: 2px;
  display: flex;
  flex-direction: column;
  padding: 30rpx 38rpx;
  box-sizing: border-box;
  overflow: auto;
  padding-bottom: 95rpx;
}
.shareRuleOutput .title{
  font-size: 34rpx;
  color: #333;
}
.shareRuleOutput .des{
  font-size: 24rpx;
  color: #4C4C4C;
  margin-top: 25rpx;
  line-height: 38rpx;
}
.shareRuleOutput .cancel{
  position: absolute;
  bottom: 0;
  left: 0;
  height: 90rpx;
  width: 100%;
  background: #477CEF;
  font-size: 32rpx;
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
}